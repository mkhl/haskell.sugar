<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <setting name="comment-multiline-start">
        <syntax-context>haskell, haskell *</syntax-context>
        <value>{-</value>
    </setting>
    <setting name="comment-multiline-end">
        <syntax-context>haskell, haskell *</syntax-context>
        <value>-}</value>
    </setting>
    <setting name="comment-singleline-start">
        <syntax-context>haskell, haskell *</syntax-context>
        <value>-- </value>
    </setting>
</settings>
